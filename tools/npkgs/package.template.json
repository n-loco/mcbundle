{
  "name": "@mcbundle/{{target_double}}",
  "version": "{{program_version}}",
  "description": "mcbundle binary for {{display_cpu}} {{display_os}}.",
  "files": [
    "debug/mcbundle{{exename}}"
  ],
  "scripts": {
    "prepack": "tools.prepack",
    "postpack": "tools.postpack"
  },
  "license": "MIT",
  "homepage": "https://github.com/n-loco/mcbundle#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/n-loco/mcbundle.git"
  },
  "bugs": {
    "url": "https://github.com/n-loco/mcbundle/issues"
  },
  "packageManager": "pnpm@10.8.1",
  "devDependencies": {
    "root": "workspace:../.."
  },
  "preferUnplugged": true,
  "os": [
    "{{os}}"
  ],
  "cpu": [
    "{{cpu}}"
  ],
  "publishConfig": {
    "executableFiles": [
      "./dist/mcbundle{{exename}}"
    ]
  }
}