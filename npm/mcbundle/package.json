{
  "name": "mcbundle",
  "version": "1.0.0-indev",
  "description": "CLI bundler for Minecraft: Bedrock Edition add-on development.",
  "type": "module",
  "files": [
    "debug/"
  ],
  "bin": "debug/mcbundle.mjs",
  "scripts": {
    "sync-package": "tools.syncv && node update_native.js",
    "prepack": "tools.prepack",
    "postpack": "tools.postpack",
    "build": "node esbuild.js"
  },
  "keywords": [
    "minecraft-bedrock-edition",
    "minecraft-bedrock",
    "minecraft-addons",
    "bedrock-addons",
    "bundler",
    "mcbundle",
    "minecraft",
    "mcpack",
    "mcaddon"
  ],
  "license": "MIT",
  "homepage": "https://github.com/n-loco/mcbundle#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/n-loco/mcbundle.git"
  },
  "bugs": {
    "url": "https://github.com/n-loco/mcbundle/issues"
  },
  "packageManager": "pnpm@10.8.1",
  "optionalDependencies": {
    "@mcbundle/darwin-arm64": "workspace:../@mcbundle/darwin-arm64",
    "@mcbundle/darwin-x64": "workspace:../@mcbundle/darwin-x64",
    "@mcbundle/linux-arm64": "workspace:../@mcbundle/linux-arm64",
    "@mcbundle/linux-ia32": "workspace:../@mcbundle/linux-ia32",
    "@mcbundle/linux-x64": "workspace:../@mcbundle/linux-x64",
    "@mcbundle/win32-arm64": "workspace:../@mcbundle/win32-arm64",
    "@mcbundle/win32-ia32": "workspace:../@mcbundle/win32-ia32",
    "@mcbundle/win32-x64": "workspace:../@mcbundle/win32-x64"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "esbuild": "^0.25.3",
    "root": "workspace:.."
  }
}